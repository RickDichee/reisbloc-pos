# Variables de Entorno - Copia este archivo a .env.local


# Supabase Configuration (migración gradual)
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Supabase Feature Flags (migración gradual - todos false inicialmente)
VITE_SUPABASE_AUTH_ENABLED=false
VITE_SUPABASE_DB_ENABLED=false
VITE_SUPABASE_STORAGE_ENABLED=false

# MercadoPago Configuration (reemplaza Clip)
VITE_MERCADOPAGO_PUBLIC_KEY=your_mercadopago_public_key
VITE_MERCADOPAGO_ACCESS_TOKEN=your_mercadopago_access_token

# App Configuration
VITE_APP_ENV=development
VITE_APP_URL=http://localhost:5173
VITE_LOG_LEVEL=info
